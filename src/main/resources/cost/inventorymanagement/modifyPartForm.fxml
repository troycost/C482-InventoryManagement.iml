      <TextField fx:id="minTxtFld" layoutX="376.0" layoutY="237.0" />
      <Label fx:id="modifyPartLabel" layoutX="34.0" layoutY="24.0" prefHeight="15.0" prefWidth="88.0" text="Modify Part">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <Button fx:id="saveButton" layoutX="357.0" layoutY="323.0" mnemonicParsing="false" onAction="#onSave" text="Save" />
      <Button fx:id="cancelButton" layoutX="448.0" layoutY="323.0" mnemonicParsing="false" onAction="#onCancel" text="Cancel" />
   </children>
</AnchorPane>
