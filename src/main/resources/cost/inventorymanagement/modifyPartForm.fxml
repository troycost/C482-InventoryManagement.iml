      <Label fx:id="nameLabel" layoutX="29.0" layoutY="119.0" text="Name:" />
      <Label fx:id="invLabel" layoutX="29.0" layoutY="160.0" text="Inv:" />
      <Label fx:id="priceCostLabel" layoutX="29.0" layoutY="200.0" text="Price/Cost:" />
      <Label fx:id="maxLabel" layoutX="29.0" layoutY="241.0" text="Max:" />
      <Label fx:id="machineIdLabel" layoutX="29.0" layoutY="278.0" text="Machine ID:" />
      <Label fx:id="minLabel" layoutX="345.0" layoutY="241.0" text="Min:" />
      <TextField fx:id="idTxtFld" editable="false" layoutX="152.0" layoutY="72.0" promptText="Auto-Generated" />
